<div class="row">


    {% for articulo in articulosLink %}
        <div class="box col-md-4">
            <div class="box-inner">
                <div class="box-header well" data-original-title="">


                   
                </div>
                <div class="box-content">
                    <div class="row">
                        <div class="col-md-4"><h6>{{articulo.nombre}}</h6></div>
                        <div class="col-md-4"><h6>{{articulo.descripcion}}</h6></div>
